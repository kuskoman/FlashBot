{
    "extends": "recommended",
    "rules": {
        "ClassJavadoc": {
            "enabled": false
        },
        "CompileStatic": {
            "enabled": false
        },
        "ExplicitHashMapInstantiation": {
            "enabled": false
        },
        "FieldName": {
            "enabled": false
        },
        "FieldTypeRequired": {
            "enabled": false
        },
        "NoDef": {
            "enabled": false
        },
        "UnnecessaryGetter": {
            "enabled": false
        },
        "UnnecessarySetter": {
            "enabled": false
        },
        "VariableTypeRequired": {
            "enabled": false
        }
    }
}